{
  "name": "vigis_pose",
  "version": "1.0.0",
  "description": "Mocap Studio com Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:win": "electron-packager . VigisPose --platform=win32 --arch=x64 --out=dist --prune=true --overwrite --icon=assets/icon.ico",
    "pack:linux": "electron-packager . VigisPose --platform=linux --arch=x64 --out=dist --prune=true --overwrite --icon=assets/icon.png",
    "pack:mac": "electron-packager . VigisPose --platform=darwin --arch=x64 --out=dist --prune=true --overwrite --icon=assets/icon.icns"
  },
  "keywords": [
    "electron",
    "mocap",
    "pose-estimation",
    "tensorflow",
    "mediapipe"
  ],
  "author": "Seu Nome",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@mediapipe/pose": "^0.5.1675469404",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs-backend-webgl": "^4.17.0",
    "@tensorflow/tfjs-core": "^4.17.0",
    "chart.js": "^4.4.2",
    "leaflet": "^1.9.4",
    "three": "^0.163.0"
  },
  "devDependencies": {
    "electron": "^29.1.5",
    "electron-packager": "^17.1.2"
  }
}